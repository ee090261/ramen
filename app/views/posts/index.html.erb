

<h1>投稿一覧</h1>

<table>
  	<thead>
		<tr>
			<th>写真</th>
			<th>メニュー</th>
			<th>価格</th>
			<th>コメント</th>
		</tr>
	</thead>

	<tbody>
	<% @posts.each do |post| %>
		<%= link_to post_path(post) do %>
           <%= attachment_image_tag post ,:dish_image, format: "jpeg", id: "img_prev", fallback: "no-image.png", class: 'img_prev' %>
		   <%= post.menu_title %>
		    <%= post.price %>
		    <%= post.comment %>
		<% end %>
	<% end %>
    </tbody>
</table>









