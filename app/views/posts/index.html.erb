

<h1>投稿一覧</h1>

<table>
  	<thead>
		<tr>
			<th>写真</th>
			<th>メニュー</th>
			<th>価格</th>
			<th>店舗名</th>
			<th>住所</th>
			<th>コメント</th>
		</tr>
	</thead>

	<tbody>
	  
		<% @posts.each do |post| %>
	      <tr>
		        <th><%= link_to post_path(post) do %>
		        	  <%= attachment_image_tag post ,:dish_image, format: "jpeg", fallback: "no-image.png", class: 'img_prev' %>
		        	<% end %>
		        </th>
				<th><%= post.menu_title %></th>
				<th><%= post.price %>円</th>
				<th><%= post.shop_name %></th>
				<th><%= post.adress %></th>
				<th><%= post.comment %></th>
		  </tr>
	    <% end %>
    </tbody>
</table>









