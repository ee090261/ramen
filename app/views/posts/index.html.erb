

<h1>投稿一覧</h1>

<table>
  	<thead>
		<tr>
			<th>写真</th>
			<th>メニュー</th>
			<th>価格</th>
			<th>コメント</th>
		</tr>
	</thead>
	<tbody>
	<% @posts.each do |post| %>
	  <tr>
		<td><%= attachment_image_tag post ,:dish_image, format: "jpeg", id: "img_prev", fallback: "no-image.png" %></td>
		<td><%= post.menu_title %></td>
		<td><%= post.price %></td>
		<td><%= post.comment %></td>
	  </tr>
	<% end %>
    </tbody>
</table>









